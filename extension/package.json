{
  "name": "in-notes",
  "version": "1.2.0",
  "private": true,
  "development": {
    "rewire": "^5.0.0"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "antd": "^5.4.4",
    "crypto-js": "^4.2.0",
    "javascript-time-ago": "^2.5.11",
    "lodash": "^4.17.21",
    "react": "^19.0.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^19.0.0",
    "react-scripts": "5.0.0",
    "react-shadow": "^20.6.0",
    "rewire": "^5.0.0",
    "styled-components": "^5.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "export NODE_OPTIONS=--openssl-legacy-provider && react-scripts build",
    "build:extension": "export NODE_OPTIONS=--openssl-legacy-provider && node ./scripts/build-non-split.js && rm -rf InNotes.zip && zip -r InNotes.zip ./build",
    "build:generateSafari": "node ./scripts/build-non-split.js && ./GenerateSafari.sh",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clean": "rm -rf ./build ./node_modules"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
